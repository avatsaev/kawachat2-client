{
  "name": "kawachat2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "sw": "cp node_modules/@angular/service-worker/bundles/worker-basic.min.js dist/service-worker.js && ./node_modules/ng-pwa-tools/bin/ngu-sw-manifest.js --module src/app/app.module.ts --out dist/ngsw-manifest.json",
    "pwa-build": "ng build --prod --aot && npm run sw && npm run app-shell",
    "app-shell": "./node_modules/ng-pwa-tools/bin/ngu-app-shell.js --url /loading --module src/app/app.module.ts --insert-module src/app/modules/loading/loading.module.ts --out dist/index.html",
    "surge-deploy": "npm run pwa-build && cp dist/index.html dist/200.html && surge --domain kawachat2.surge.sh --project ./dist ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~4.0.3",
    "@angular/common": "~4.0.3",
    "@angular/compiler": "~4.0.3",
    "@angular/core": "~4.0.3",
    "@angular/forms": "~4.0.3",
    "@angular/http": "~4.0.3",
    "@angular/platform-browser": "~4.0.3",
    "@angular/platform-browser-dynamic": "~4.0.3",
    "@angular/platform-server": "~4.0.3",
    "@angular/router": "~4.0.3",
    "@angular/service-worker": "^1.0.0-beta.15",
    "@types/socket.io-client": "^1.4.29",
    "angular2-uuid": "^1.1.1",
    "bulma": "^0.4.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "ng-pwa-tools": "^0.0.15",
    "ngx-webstorage": "^1.8.0",
    "request-promise": "^4.2.1",
    "rxjs": "^5.1.0",
    "socket.io-client": "^2.0.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.1.3",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
